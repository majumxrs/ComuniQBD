            @Html.DisplayNameFor(model => model.TipoCampanha)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TipoCampanha.TipoCampanhaNome)
        </dd>
        <dt class = "col-sm-2">
