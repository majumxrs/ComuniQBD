@model ComuniQBD.Models.Campanha

@{
    ViewData["Title"] = "Details";
}

<h1>Detalhes</h1>

<div>
    <h4>Campanha</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CampanhaTitulo)
        </dt>
        <dd class = "col-sm-10">
